<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!--DS-->
    <class name="com.greatbee.core.bean.oi.DS" table="ty_ds" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="name" column="`name`" type="string"/>
        <property name="description" column="`description`" type="string"/>
        <property name="alias" column="alias" type="string"/>
        <property name="dst" column="dst" type="string"/>
        <property name="connectionUrl" column="`connection_url`" type="string"/>
        <property name="connectionUsername" column="`connection_username`" type="string"/>
        <property name="connectionPassword" column="`connection_password`" type="string"/>
        <property name="dsConfigFrom" column="`ds_config_from`" type="string"/>
        <property name="tddlAppName" column="`tddl_appname`" type="string"/>
    </class>

    <!-- OI-->
    <class name="com.greatbee.core.bean.oi.OI" table="ty_oi" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="name" column="`name`" type="string"/>
        <property name="description" column="description" type="string"/>
        <property name="alias" column="alias" type="string"/>
        <property name="resource" column="resource" type="string"/>
        <property name="dsAlias" column="`ds_alias`" type="string"/>
    </class>

    <!-- Field-->
    <class name="com.greatbee.core.bean.oi.Field" table="ty_field" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="name" column="`name`" type="string"/>
        <property name="description" column="description" type="string"/>
        <property name="fieldName" column="`field_name`" type="string"/>
        <property name="oiAlias" column="`oi_alias`" type="string"/>
        <property name="dt" column="dt" type="string"/>
        <property name="parentFieldId" column="`parent_field_id`" type="integer"/>
        <!--private List<Field> fields;-->
        <property name="pk" column="pk" type="boolean"/>
        <property name="fieldValue" column="`field_value`" type="string"/>
        <property name="fieldLength" column="`field_length`" type="integer"/>
        <property name="group" column="`f_group`" type="string"/>
        <property name="uuid" column="`uuid`" type="string"/>
    </class>

    <!-- Connector-->
    <class name="com.greatbee.core.bean.oi.Connector" table="ty_connector" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="name" column="`name`" type="string"/>
        <property name="description" column="description" type="string"/>
        <property name="alias" column="alias" type="string"/>
        <property name="fromOIAlias" column="`from_oi_alias`" type="string"/>
        <property name="fromFieldName" column="`from_field_name`" type="string"/>
        <property name="toOIAlias" column="`to_oi_alias`" type="string"/>
        <property name="toFieldName" column="`to_field_name`" type="string"/>
    </class>
</hibernate-mapping>